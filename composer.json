{
  "name": "orumad/laravel-app-version",
  "description": "Update app version in 'package.json' and 'composer.json' files. Add commit message and version to the CHANGELOG.md file. Commit changes and (optionaly) deploy them.",
  "type": "library",
  "version": "1.0.3",
  "keywords": [
    "laravel",
    "package",
    "git",
    "deploy",
    "orumad"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Mu√±oz",
      "email": "dev@danielmunoz.io"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Orumad\\LaravelAppVersion\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Orumad\\LaravelAppVersion\\LaravelAppVersionServiceProvider"
      ]
    }
  }
}
